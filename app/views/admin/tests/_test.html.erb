 <tr>
  <td class="text-center"><%= test.id %></td>
  <td><%= test.title %></td>
  <td><%= test_level(test) %></td>
  <td class="text-center"><%= test.questions.count %></td>
  <td ><%= link_to 'Show', admin_test_path(test), class: "badge badge-info" %></td>
  <td><%= link_to 'Edit', edit_admin_test_path(test), class: "badge badge-warning" %></td>
  <td><%= link_to 'Delete',
                  admin_test_path(test),
                  method: :delete,
                  data:  { confirm: 'Are you sure?' },
                  class: "badge badge-danger" %></td>
</tr>
