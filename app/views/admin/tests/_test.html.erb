 <tr>
  <td class="text-center"><%= test.id %></td>
  <td><%= test.title %></td>
  <td><%= test_level(test) %></td>
  <td class="text-center"><%= test.questions.count %></td>
  <td ><%= link_to t('show_action'), admin_test_path(test), class: "badge badge-info" %></td>
  <td><%= link_to t('edit_action'), edit_admin_test_path(test), class: "badge badge-warning" %></td>
  <td><%= link_to t('delete_action'),
                  admin_test_path(test),
                  method: :delete,
                  data:  { confirm: t('.confirmation') },
                  class: "badge badge-danger" %></td>
</tr>
